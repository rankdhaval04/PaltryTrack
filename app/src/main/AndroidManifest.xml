<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="paltrytrack.maker.paltrytrack">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/drawer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".dashboard"
            android:label="Dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".search"
            android:label="@string/title_activity_search"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".profile"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".placeorder"
            android:label="Product" />
        <activity
            android:name=".storeproduct"
            android:label="Store Product" />
        <activity
            android:name=".placeorder1"
            android:label="Product" />
        <activity android:name=".Register" />
        <activity
            android:name=".cart"
            android:label="Cart" />
        <activity
            android:name=".billg"
            android:label="Bill" />
        <activity android:name=".placeorder3" />
        <activity
            android:name=".editprofile"
            android:label="Edit Profile" />
        <activity android:name=".address" />
        <activity
            android:name=".fav_stores"
            android:label="@string/title_activity_fav_stores"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>